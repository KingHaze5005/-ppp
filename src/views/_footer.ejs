<!-- RANDOM ADVERTISEMENT START -->
<% if(randomad != 'none') { %>
    <div class="container">
        <a href="<%= randomad.link %>" target="_blank">
            <img src="/images/advertisement_<%= randomad.uniqueid %>.<%= randomad.filetype %>" class="footeradvertisement bd-theme" alt="random advertisement image" loading="lazy">
        </a>
    </div>
    <br>
<% }; %>
<!-- RANDOM ADVERTISEMENT END -->
<!-- FOOTER START -->
<div class="footerimage bg-thatdark">
    <div class="container-fluid">
        <div class="row justify-content-center text-start">
            <div class="col-lg-4 footercolumnscentermobile">
                <h3 class="bigfont mt-5 mb-3"><%= sitesettings.sitename %></h3>
                <p class="footerdescone mb-5"><%= sitesettings.sitedesc %></p>
                <small>Copyright &copy; <%= currentyear %> <%= sitesettings.sitename %> - All rights reserved.</small>
                <div class="mt-4">
                    <% if(sitesettings.email != 'none') { %>
                        <a class="socialicon" href="mailto:<%= sitesettings.email %>" target="_blank"><i class="fa-solid fa-envelope"></i></a>
                    <% }; %>
                    <% if(sitesettings.twitter != 'none') { %>
                        <a class="socialicon" href="https://twitter.com/<%= sitesettings.twitter %>" target="_blank"><i class="fa-brands fa-twitter"></i></a>
                    <% }; %>
                    <% if(sitesettings.discord != 'none') { %>
                        <a class="socialicon" href="<%= sitesettings.discord %>" target="_blank"><i class="fa-brands fa-discord"></i></a>
                    <% }; %>
                    <% if(sitesettings.youtube != 'none') { %>
                        <a class="socialicon" href="<%= sitesettings.youtube %>" target="_blank"><i class="fa-brands fa-youtube"></i></a>
                    <% }; %>
                    <% if(sitesettings.instagram != 'none') { %>
                        <a class="socialicon" href="https://instagram.com/<%= sitesettings.instagram %>" target="_blank"><i class="fa-brands fa-instagram"></i></a>
                    <% }; %>
                    <% if(sitesettings.facebook != 'none') { %>
                        <a class="socialicon" href="<%= sitesettings.facebook %>" target="_blank"><i class="fa-brands fa-facebook"></i></a>
                    <% }; %>
                    <% if(sitesettings.tiktok != 'none') { %>
                        <a class="socialicon" href="https://tiktok.com/@<%= sitesettings.tiktok %>" target="_blank"><i class="fa-brands fa-tiktok"></i></a>
                    <% }; %>
                </div>
            </div>
            <% if(sitestyles.footnavigation) { %>
                <div class="col-lg-2 footercolumnscentermobile">
                    <p class="footerheaderone">Navigation</p>
                    <p class="footeritemone"><a href="/">Home</a></p>
                    <p class="footeritemone"><a href="/shop">Shopping</a></p>
                    <p class="footeritemone"><a href="/reviews">Reviews</a></p>
                    <p class="footeritemone"><a href="/account">Account</a></p>
                </div>
            <% }; %>
            <% if(sitestyles.footusefullinks) { %>
                <div class="col-lg-2 footercolumnscentermobile">
                    <p class="footerheaderone">Useful Links</p>
                    <p class="footeritemone"><a href="/giftcards">Gift Cards</a></p>
                    <p class="footeritemone"><a href="/discord">Our Discord</a></p>
                    <p class="footeritemone"><a href="/faq">FAQs</a></p>
                </div>
            <% }; %>
            <% if(sitestyles.footourpartners) { %>
                <div class="col-lg-2 footercolumnscentermobile">
                    <p class="footerheaderone">Our Partners</p>
                    <% partners.forEach(function(item) { %>
                        <p class="footeritemone"><a href="<%= item.link %>"><%= item.title %></a></p>
                <% }); %>
                </div>
            <% }; %>
            <% if(sitestyles.footlegal) { %>
                <div class="col-lg-2 footercolumnscentermobile">
                    <p class="footerheaderone">Legal</p>
                    <p class="footeritemone"><a href="/tos">Terms of Service</a></p>
                    <p class="footeritemone"><a href="/privacy">Privacy Policy</a></p>
                    <p class="footeritemone"><a href="/cookies">Cookie Policy</a></p>
                </div>
            <% }; %>
        </div>
        <div class="row justify-content-center">
            <div class="col-12 footercolumnscentermobile text-end">
                <p style="font-size: smaller;" class="mt-3">Produced by <a href="https://huracan.solutions" class="text-theme" target="_blank">Huracan Solutions</a> & <a href="https://github.com/itz-hyperz" class="text-theme" target="_blank">ThatGuyHyperz</a></p>
            </div>
        </div>
        <br>
    </div>
</div>
<!-- FOOTER END -->
