<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Terms of Service - <%= sitesettings.sitename %></title>
        <meta property="og:title" content="Terms of Service - <%= sitesettings.sitename %>" />
        <link rel="icon" href="/assets/logo.png">
        <meta property="og:image" content="/assets/logo.png">
        <meta name="theme-color" content="<%= sitesettings.sitecolor %>">
        <meta name="description" content="<%= sitesettings.sitedesc %>" />
        <meta property="og:type" content="website" />
        <meta property="og:description" content="<%= sitesettings.sitedesc %>" />
    </head>
    <body>
        <%- include('_header') %>
        <div class="container text-start mb-4">
            <h1 class="bigfont text-start"><%= sitesettings.sitename %>'s Terms and Conditions Policy</h1>
            <hr>
            <br>
            <%- converted %>
        </div>
        <%- include('_footer') %>
    </body>
</html>