<%
  function convert(value) {
    if(value) {
      return "Yes";
    } else {
      return "No";
    };
  };
  function convertToEorD(value) {
    if(value) {
      return "Enabled";
    } else {
      return "Disabled";
    };
  };
  function convertToWord(value) {
    switch(value) {
      case 0:
        return 'Disabled';
        break;
      case 1:
        return 'One';
        break;
      case 2:
        return 'Two';
        break;
      case 3:
        return 'Three';
        break;
      case 4:
        return 'Four';
        break;
      case 5:
        return 'Five';
        break;
      case 6:
        return 'Six';
        break;
    };
  };
  function convertToWordLib(value) {
    switch(value) {
      case 1:
        return 'One';
        break;
      case 2:
        return 'Liberitarian';
        break;
    };
  };
  function convertStaffRanking(value) {
    switch(value) {
      case 1:
        return 'Viewer';
        break;
      case 2:
        return 'Developer';
        break;
      case 3:
        return 'Staff';
        break;
      case 4:
        return 'Owner';
        break;
    };
  };
  function convertToText(item) {
        if(item) {
            return 'Yes';
        } else {
            return 'No';
        };
  };
  data.products = data.products.sort((a, b) => {
    return a.pos - b.pos;
  });
  function convertInvoicePaid(value) {
    if(value) {
      return 'PAID';
    } else {
        return 'UNPAID';
    };
  };
  let stats_customers = Object.values(data.statistics.filter(a => a.type == 'customers')[0]);
  let stats_autojoin = Object.values(data.statistics.filter(a => a.type == 'autojoin')[0]);
  let stats_sales = Object.values(data.statistics.filter(a => a.type == 'sales')[0]);
  let stats_income = Object.values(data.statistics.filter(a => a.type == 'income')[0]);
  let stats_homevisits = Object.values(data.statistics.filter(a => a.type == 'homepagevisits')[0]);
  let stats_newusers = Object.values(data.statistics.filter(a => a.type == 'newusers')[0]);
  stats_customers.shift();
  stats_autojoin.shift();
  stats_sales.shift();
  stats_income.shift();
  stats_homevisits.shift();
  stats_newusers.shift();
%>

